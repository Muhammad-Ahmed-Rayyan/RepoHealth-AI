{
  "name": "repohealth-ai",
  "version": "1.0.0",
  "description": "GitHub Repository Health Dashboard with AI-powered improvement suggestions",
  "private": true,
  "scripts": {
    "install-all": "npm install && cd client && npm install && cd ../server && npm install",
    "dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:client": "cd client && npm run dev",
    "start:server": "cd server && npm run dev",
    "build": "cd client && npm run build"
  },
  "keywords": [
    "github",
    "repository",
    "health",
    "dashboard",
    "ai",
    "analytics"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
